[
  {
    "essential": true,
    "memory": 256,
    "image": "643009352547.dkr.ecr.us-west-2.amazonaws.com/infra/vault:0.9.3",
    "name": "vault",
    "linuxParameters": {
      "capabilities": {
        "add": [
          "IPC_LOCK"
        ]
      }
    },
    "portMappings": [
      {
        "containerPort": 8200,
        "hostPort": 8200,
        "protocol": "tcp"
      },
      {
        "containerPort": 8201,
        "hostPort": 8201,
        "protocol": "tcp"
      }
    ],
    "environment": [
      {
        "name": "VAULT_REDIRECT_INTERFACE",
        "value": "eth0"
      },
      {
        "name": "VAULT_CLUSTER_INTERFACE",
        "value": "eth0"
      }
    ],
    "mountPoints": [
      {
        "sourceVolume": "vault-logs-storage",
        "containerPath": "/vault/logs"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "infra",
        "awslogs-region": "us-west-2",
        "awslogs-stream-prefix": "vault"
      }
    },
    "dockerLabels": {
      "app_name": "vault",
      "app_env": "production",
      "app_region": "${AWS_REGION}"
    }
  }
]