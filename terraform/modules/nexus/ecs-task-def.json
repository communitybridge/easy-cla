[
  {
    "essential": true,
    "image": "sonatype/nexus3:3.8.0",
    "memoryReservation": 1528,
    "name": "nexus",
    "portMappings": [{
      "containerPort": 8081,
      "hostPort": 8081
    }],
    "ulimits": [
      {
        "name": "nofile",
        "softLimit": 65536,
        "hardLimit": 65536
      }
    ],
    "mountPoints": [
      {
        "sourceVolume": "nexus-storage",
        "containerPath": "/nexus-data"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "infra",
        "awslogs-region": "us-west-2",
        "awslogs-stream-prefix": "nexus"
      }
    },
    "dockerLabels": {
      "app_name": "nexus",
      "app_env": "production",
      "app_region": "${TAG_REGION}"
    }
  }
]