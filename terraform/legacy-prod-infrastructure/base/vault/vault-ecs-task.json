[
  {
    "essential": true,
    "memory": 256,
    "image": "643009352547.dkr.ecr.us-west-2.amazonaws.com/infrastructure/vault:latest",
    "name": "vault-server",
    "privileged": true,
    "portMappings": [
      {
        "containerPort": 8200,
        "hostPort": 8200,
        "protocol": "tcp"
      },
      {
        "containerPort": 8201,
        "hostPort": 8201,
        "protocol": "tcp"
      }
    ],
    "dnsServers": [
      "${DNS_SERVER_1}",
      "${DNS_SERVER_2}",
      "${DNS_SERVER_3}"
    ],
    "environment": [
      {
        "name": "CONSUL_MASTER_IP",
        "value": "${CONSUL_MASTER_IP}"
      },
      {
        "name": "SERVICE_IGNORE",
        "value": "true"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "infrastructure",
        "awslogs-region": "us-west-2",
        "awslogs-stream-prefix": "vault"
      }
    },
    "dockerLabels": {
      "app_name": "vault",
      "app_env": "production",
      "app_region": "${AWS_REGION}"
    }
  }
]