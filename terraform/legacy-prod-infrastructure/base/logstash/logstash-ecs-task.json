[
  {
    "essential": true,
    "image": "643009352547.dkr.ecr.us-west-2.amazonaws.com/infrastructure/logstash:latest",
    "memoryReservation": 1024,
    "name": "logstash",
    "portMappings": [
      {
        "containerPort": 5044,
        "hostPort": 5044
      },
      {
        "containerPort": 12201,
        "hostPort": 12201,
        "protocol": "udp"
      },
      {
        "containerPort": 9600,
        "hostPort": 9600
      },
      {
        "containerPort": 1514,
        "hostPort": 1514
      }
    ],
    "dnsServers": [
      "${DNS_SERVER_1}",
      "${DNS_SERVER_2}",
      "${DNS_SERVER_3}"
    ],
    "environment": [
      {
        "name": "PIPELINE_NAME",
        "value": "${PIPELINE_NAME}"
      },
      {
        "name": "AWS_ACCESS_KEY_ID",
        "value": "${AWS_ACCESS_KEY_ID}"
      },
      {
        "name": "AWS_SECRET_ACCESS_KEY",
        "value": "${AWS_SECRET_ACCESS_KEY}"
      },
      {
        "name": "S3_LOG_BUCKET",
        "value": "${S3_LOG_BUCKET}"
      },
      {
        "name": "ES_USERNAME",
        "value": "${ES_USERNAME}"
      },
      {
        "name": "ES_PASSWORD",
        "value": "${ES_PASSWORD}"
      },
      {
        "name": "ES_HOST_URL",
        "value": "${ES_HOST_URL}"
      },
      {
        "name": "SERVICE_9600_NAME",
        "value": "logstash"
      },
      {
        "name": "SERVICE_9600_TAGS",
        "value": "${TAG_REGION},${TAG_VPC_ID}"
      },
      {
        "name": "SERVICE_9600_CHECK_TCP",
        "value": "true"
      },
      {
        "name": "SERVICE_9600_CHECK_INTERVAL",
        "value": "5s"
      },
      {
        "name": "SERVICE_12201_IGNORE",
        "value": "true"
      },
      {
        "name": "SERVICE_5044_IGNORE",
        "value": "true"
      },
      {
        "name": "SERVICE_1514_IGNORE",
        "value": "true"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "infrastructure",
        "awslogs-region": "us-west-2",
        "awslogs-stream-prefix": "logstash"
      }
    }
  }
]