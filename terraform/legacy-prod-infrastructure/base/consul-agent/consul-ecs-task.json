[
  {
    "essential": true,
    "memory": 256,
    "image": "643009352547.dkr.ecr.us-west-2.amazonaws.com/consul-agent:latest",
    "name": "consul-agent",
    "dnsServers": [
      "${DNS_SERVER_1}",
      "${DNS_SERVER_2}",
      "${DNS_SERVER_3}"
    ],
    "environment": [
      {
        "name": "CONSUL_ENCRYPT",
        "value": "${ENCRYPTION}"
      },
      {
        "name": "CONSUL_DATACENTER",
        "value": "${DATACENTER}"
      },
      {
        "name": "CONSUL_ENDPOINT",
        "value": "${CONSUL_ENDPOINT}"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "infrastructure",
        "awslogs-region": "us-west-2",
        "awslogs-stream-prefix": "consul-agent"
      }
    },
    "dockerLabels": {
      "app_name": "consul-agent",
      "app_env": "production",
      "app_region": "${AWS_REGION}"
    }
  }
]