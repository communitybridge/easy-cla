[
  {
    "essential": true,
    "memory": 256,
    "image": "djenriquez/vault-ui:latest",
    "name": "vault-ui",
    "privileged": true,
    "portMappings": [
      {
        "containerPort": 8000
      }
    ],
    "dnsServers": [
      "${DNS_SERVER_1}",
      "${DNS_SERVER_2}",
      "${DNS_SERVER_3}"
    ],
    "environment": [
      {
        "name": "VAULT_URL_DEFAULT",
        "value": "${VAULT_URL_DEFAULT}"
      },
      {
        "name": "VAULT_AUTH_DEFAULT",
        "value": "${VAULT_AUTH_DEFAULT}"
      }
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "infrastructure",
        "awslogs-region": "us-west-2",
        "awslogs-stream-prefix": "vault-ui"
      }
    },
    "dockerLabels": {
      "app_name": "vault",
      "app_env": "production",
      "app_region": "${AWS_REGION}"
    }
  }
]