[
  {
    "essential": true,
    "memory": 256,
    "image": "gliderlabs/registrator:latest",
    "name": "registrator",
    "command": ["-cleanup", "consul://127.0.0.1:8500"],
    "mountPoints": [
      {
        "sourceVolume": "docker-sock",
        "containerPath": "/tmp/docker.sock"
      }
    ],
    "dnsServers": [
      "${DNS_SERVER_1}",
      "${DNS_SERVER_2}",
      "${DNS_SERVER_3}"
    ],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "infrastructure",
        "awslogs-region": "us-west-2",
        "awslogs-stream-prefix": "registrator"
      }
    }
  }
]