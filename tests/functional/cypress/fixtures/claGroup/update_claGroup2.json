{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "ccla_enabled": {
        "type": "boolean"
      },
      "ccla_pdf_url": {
        "type": "string"
      },
      "ccla_requires_icla": {
        "type": "boolean"
      },
      "cla_group_description": {
        "type": "string"
      },
      "cla_group_id": {
        "type": "string"
      },
      "cla_group_name": {
        "type": "string"
      },
      "foundationLevelCLA": {
        "type": "boolean"
      },
      "foundation_name": {
        "type": "string"
      },
      "foundation_sfid": {
        "type": "string"
      },
      "icla_enabled": {
        "type": "boolean"
      },
      "icla_pdf_url": {
        "type": "string"
      },
      "project_list": {
        "type": "array",
        "items": [
          {
            "type": "object",
            "properties": {
              "project_name": {
                "type": "string"
              },
              "project_sfid": {
                "type": "string"
              },
              "repositories_count": {
                "type": "integer"
              }
            },
            "required": [
              "project_name",
              "project_sfid",
              "repositories_count"
            ]
          }
        ]
      },
      "repositories_count": {
        "type": "integer"
      },
      "root_project_repositories_count": {
        "type": "integer"
      },
      "setup_completion_pct": {
        "type": "null"
      },
      "total_signatures": {
        "type": "integer"
      }
    },
    "required": [
      "ccla_enabled",
      "ccla_pdf_url",
      "ccla_requires_icla",
      "cla_group_description",
      "cla_group_id",
      "cla_group_name",
      "foundationLevelCLA",
      "foundation_name",
      "foundation_sfid",
      "icla_enabled",
      "icla_pdf_url",
      "project_list",
      "repositories_count",
      "root_project_repositories_count",
      "setup_completion_pct",
      "total_signatures"
    ]
  }