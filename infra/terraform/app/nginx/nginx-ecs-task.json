[
  {
    "essential": true,
    "image": "643009352547.dkr.ecr.us-west-2.amazonaws.com/pmc/nginx:${build_hash}",
    "memoryReservation": 258,
    "name": "nginx",
    "portMappings": [{
      "containerPort": 80
    }],
    "dnsServers": [
      "${DNS_SERVER_1}",
      "${DNS_SERVER_2}",
      "${DNS_SERVER_3}"
    ],
    "environment": [
      {
        "name": "APP_DOMAINS",
        "value": "${APP_DOMAINS}"
      },
      {
        "name": "CONSUL_SERVICE",
        "value": "${CONSUL_SERVICE}"
      },
      {
        "name": "SERVICE_80_NAME",
        "value": "PMC_NGINX_${build_hash}"
      },
      {
        "name": "SERVICE_80_TAGS",
        "value": "${TAG_REGION},${TAG_VPC_ID}"
      }
    ],
    "logConfiguration": {
      "logDriver": "gelf",
      "options": {
        "gelf-address": "udp://${TAG_REGION}.logstash.service.consul:12201",
        "labels": "realm,application,version,environment,region,tags"
      }
    },
    "dockerLabels": {
      "realm": "pmc",
      "application": "nginx",
      "version": "${build_hash}",
      "environment": "production",
      "region": "${TAG_REGION}",
      "tags": "application, nginx, proxy"
    }
  }
]