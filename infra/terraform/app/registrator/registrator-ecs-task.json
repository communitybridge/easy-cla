[
  {
    "essential": true,
    "memory": 256,
    "image": "gliderlabs/registrator:latest",
    "name": "registrator",
    "command": ["-cleanup", "consul://127.0.0.1:8500"],
    "mountPoints": [
      {
        "sourceVolume": "docker-sock",
        "containerPath": "/tmp/docker.sock"
      }
    ],
    "dnsServers": [
      "${DNS_SERVER_1}",
      "${DNS_SERVER_2}",
      "${DNS_SERVER_3}"
    ],
    "logConfiguration": {
      "logDriver": "gelf",
      "options": {
        "gelf-address": "udp://logstash.service.consul:12201",
        "labels": "realm,application,environment,tags"
      }
    },
    "dockerLabels": {
      "realm": "pmc",
      "application": "registrator",
      "environment": "production",
      "tags": "service, registrator"
    }
  }
]