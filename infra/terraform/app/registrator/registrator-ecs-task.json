[
  {
    "essential": true,
    "memory": 256,
    "image": "gliderlabs/registrator:latest",
    "name": "registrator",
    "command": ["-cleanup", "consul://127.0.0.1:8500"],
    "mountPoints": [
      {
        "sourceVolume": "docker-sock",
        "containerPath": "/tmp/docker.sock"
      }
    ],
    "dnsServers": [
      "${DNS_SERVER_1}",
      "${DNS_SERVER_2}",
      "${DNS_SERVER_3}"
    ],
    "logConfiguration": {
      "logDriver": "gelf",
      "options": {
        "gelf-address": "udp://us-west-2.logstash.service.consul:12201",
        "labels": "app_name,app_env,app_region"
      }
    },
    "dockerLabels": {
      "app_name": "registrator",
      "app_env": "production",
      "app_region": "${AWS_REGION}"
    }
  }
]