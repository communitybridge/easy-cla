    <div class="col-sm-3"><div name="marketing_contact_email"><%= memberCompany.contacts.marketing.email %></div></div>
    <div class="col-sm-2"><div name="marketing_contact_phone"><%= memberCompany.contacts.marketing.phone %></div></div>
    <div class="col-sm-1"><input name="marketing_contact_email_alias" type="text" class="domain-project-edit" placeholder="Select"></div>
    <div class="col-sm-1"><img src="/uploads/<%= memberCompany.contacts.marketing.headshotRef %>" alt="" style="height:50px;" /></div>
  </div>
  <br><br><br>
  <div class="hr-divider-keyline"></div>
  <br>
  <div class="subtitle-content-project col-sm-12">
    <div class="col-sm-2 table-contact-add-company">FINANCE</div>
    <div class="col-sm-1">
      <a href="#read-bio-modal" data-toggle="modal" data-bio="<%= memberCompany.contacts.finance.bio %>" data-member-type="Finance Member">View</a>
    </div>
    <div class="col-sm-1"><div name="finance_contact_first_name"><%= memberCompany.contacts.finance.givenName %></div></div>
    <div class="col-sm-1"><div name="finance_contact_last_name"><%= memberCompany.contacts.finance.familyName %></div></div>
    <div class="col-sm-3"><div name="finance_contact_email"><%= memberCompany.contacts.finance.email %></div></div>
