    <div class="col-sm-1"><input name="finance_contact_email_alias" type="text" class="domain-project-edit" placeholder="Select"></div>
    <div class="col-sm-1"><img src="/uploads/<%= memberCompany.contacts.finance.headshotRef %>" alt="" style="height:50px;" /></div>
  </div>
  <br><br><br>
  <div class="hr-divider-keyline"></div>
  <br>
  <div class="subtitle-content-project col-sm-12">
    <div class="col-sm-2 table-contact-add-company"><div name="other_contact_type"><%= memberCompany.contacts.other.type %></div></div>
    <div class="col-sm-1">
      <a href="#read-bio-modal" data-toggle="modal" data-bio="<%= memberCompany.contacts.other.bio %>" data-member-type="<%= memberCompany.contacts.other.type %> Member">View</a>
    </div>
    <div class="col-sm-1"><div name="other_contact_first_name"><%= memberCompany.contacts.other.givenName %></div></div>
    <div class="col-sm-1"><div name="other_contact_last_name"><%= memberCompany.contacts.other.familyName %></div></div>
    <div class="col-sm-3"><div name="other_contact_email"><%= memberCompany.contacts.other.email %></div></div>
    <div class="col-sm-2"><div name="other_contact_phone"><%= memberCompany.contacts.other.phone %></div></div>
    <div class="col-sm-1"><input name="other_contact_email_alias" type="text" class="domain-project-edit" placeholder="Select"></div>
    <div class="col-sm-1"><img src="/uploads/<%= memberCompany.contacts.other.headshotRef %>" alt="" style="height:50px;" /></div>
  </div>
  <br><br><br>
  <div class="hr-divider-keyline"></div>
</div>

