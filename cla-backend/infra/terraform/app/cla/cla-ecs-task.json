[
  {
    "essential": true,
    "image": "643009352547.dkr.ecr.us-west-2.amazonaws.com/cla/production:${build_hash}",
    "memoryReservation": 512,
    "name": "cla",
    "portMappings": [
      {
        "containerPort": 5000
      }
    ],
    "dnsServers": [
      "${DNS_SERVER_1}",
      "${DNS_SERVER_2}",
      "${DNS_SERVER_3}"
    ],
    "environment": [
      {
        "name": "SERVICE_5000_NAME",
        "value": "CLA_${build_hash}"
      },
      {
        "name": "SERVICE_5000_TAGS",
        "value": "${TAG_REGION},${TAG_VPC_ID}"
      }
    ],
    "logConfiguration": {
      "logDriver": "gelf",
      "options": {
        "gelf-address": "udp://${TAG_REGION}.logstash.service.consul:12201",
        "labels": "realm,application,version,environment,region,tags"
      }
    },
    "dockerLabels": {
      "realm": "cla",
      "application": "cla",
      "version": "${build_hash}",
      "environment": "production",
      "region": "${TAG_REGION}",
      "tags": "application, cla, java"
    }
  }
]